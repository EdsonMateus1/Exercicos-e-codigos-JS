<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Tabela</title>
  </head>
  <body>
    <table>
      <thead>
        <% header.forEach(e =>{ %>
        <th><%= e %></th>
        <% }) %>
      </thead>
      <tbody>
        <% rows.forEach(e =>{ %>
        <tr>
          <td><%= e %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
